const accountSid = 'ACed4bdf952a823ab5970cbc922c0cea72';
const authToken = '[AuthToken]';
const client = require('twilio')(accountSid, authToken);

client.messages
    .create({
        body: 'hello this is my thing brother',
        from: '+15344447273',
        to: '+919372584027'
    })
    .then(message => console.log(message.sid))
    .done();